{
    "name": "snippet-master",
    "displayName": "Snippets Master",
    "description": "Snippets for multiple languages",
    "version": "0.1.2",
    "author": "Slash",
    "publisher": "SlashDEV",
    "license": "MIT",
    "repository": "https://github.com/isplashy/snippetsmaster",
    "icon": "images/sm.png",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/js.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/react.code-snippets"
            }
            {
                "language": "python",
                "path": "./snippets/py.code-snippets"
            },
            {
                "language": "html",
                "path": "./snippets/ht.code-snippets"
            },
            {
                "language": "css",
                "path": "./snippets/css.code-snippets"
            }
        ]
    }
}
